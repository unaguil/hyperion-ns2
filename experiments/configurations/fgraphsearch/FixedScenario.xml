<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.5" type="% nodes with parameters">
	<staticConfig>
		<!-- Script configuration variables -->
		<entry key="nNodes">20</entry>
		<entry key="finishTime">50.0</entry>
		<entry key="gridW">290</entry>
		<entry key="gridH">290</entry>
		<entry key="discardTime">0.0</entry>
		<entry key="javaAgent">graphsearch.ForwardPeer</entry>

		<!-- Random waypoint configuration -->
		<entry key="mobilityModel">RandomWaypoint</entry>
		<entry key="minSpeed">0.00000001</entry>
		<entry key="maxSpeed">0.00000001</entry>
		<entry key="pauseTime">1000.0</entry>

		<!-- Composition populator -->
		<entry key="populator">CompositionsPopulator</entry>
		<entry key="nCompositions">1</entry>
		<entry key="compositionIO">(1, 1)</entry>
		<entry key="compositionLength">3</entry>
		<entry key="compositionWidth">5</entry>
		<entry key="nDistribution">0.3</entry>

		<!-- Search behavior -->
		<entry key="nodeBehavior">CompositionSearchBehavior</entry>
		<entry key="searchFreq">1.0</entry>
		<entry key="timeRange">(10.0, 20.0)</entry>

		<!-- Peer configuration variables -->
		<entry key="beaconDetector.beaconTime">2000</entry>
		<entry key="parameterSearch.sameTTL">20</entry>
		<entry key="graphsearch.maxTTL">10</entry>
		<entry key="graphsearch.searchExpiration">20000</entry>
	</staticConfig>
	<dynamicConfig />
	<repeat number="4" />
	<measures>
		<import file="AllMeasures.xml" />
	</measures>
</experiment>
