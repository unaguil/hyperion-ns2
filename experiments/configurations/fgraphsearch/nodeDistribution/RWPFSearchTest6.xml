<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.6" type="% nodes with services">
	<staticConfig>
		<!-- Script configuration variables -->
                <entry key="nNodes">20</entry>
                <entry key="finishTime">50.0</entry>
                <entry key="gridW">290</entry>
                <entry key="gridH">290</entry>
                <entry key="discardTime">0</entry>
                <entry key="javaAgent">graphsearch.ForwardPeer</entry>

                <!-- Random waypoint configuration -->
                <entry key="mobilityModel">RandomWaypoint</entry>
                <entry key="minSpeed">0.001</entry>
                <entry key="maxSpeed">0.001</entry>
                <entry key="pauseTime">100.0</entry>

		<!-- Composition populator -->
		<entry key="populator">CompositionsPopulator</entry>
		<entry key="nCompositions">10</entry>
		<entry key="compositionIO">(1, 5)</entry>
		<entry key="dDistribution">((0.3, 5), (0.4, 4), (0.3, 2))</entry>
		<entry key="nDistribution">0.6</entry>
		<entry key="width">(1, 4)</entry>

		<!-- Search behavior -->
		<entry key="nodeBehavior">CompositionSearchBehavior</entry>
		<entry key="searchFreq">0.1</entry>
		<entry key="timeRange">(3.0, 'END')</entry>

		<!-- Peer configuration variables -->
		<entry key="messageProcessor.randomWait">200</entry>
		<entry key="beaconDetector.beaconTime">1000</entry>
		<entry key="reliableBroadcast.maxTries">3</entry>
		<entry key="dissemination.maxDistance">5</entry>
		<entry key="parameterSearch.searchMessageTTL">5</entry>
		<entry key="graphsearch.maxTTL">10</entry>
		<entry key="graphsearch.searchExpiration">20000</entry>
	</staticConfig>
	<dynamicConfig />
	<repeat number="4" />
	<measures>
		<measure type="generic.SentMessages" period="0.25" />
		<measure type="generic.ReceivedMessages" period="0.25" />
		<measure type="detection.AvgNeighbors" period="0.25" />
		<measure type="generic.ReliableBroadcastedMessages" period="0.25" />
		<measure type="generic.DeliveredMessages" period="0.25" />
		<measure type="generic.ExpiredMessages" period="0.25" />
		<measure type="generic.DeliveredRatio" period="0.25" />
		<measure type="graphsearch.StartedCompositions" period="0.25" />
		<measure type="graphsearch.AvgFoundCompositions" period="0.25" />
		<measure type="graphsearch.AvgCompositionSearchTime" period="0.25" />
		<measure type="graphsearch.AvgCompositionAvailableTime" period="0.25" />
		<measure type="graphsearch.SentCompositionModificationMessages" period="0.25" />
		<measure type="graphsearch.SentFCompositionMessages" period="0.25" />
		<measure type="graphsearch.SentInvalidCompositionsMessages" period="0.25" />
	</measures>
</experiment>
