<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.5" type="% nodes with parameters">
	<staticConfig>
		<!-- Script configuration variables -->
		<entry key="nNodes">4</entry>
		<entry key="finishTime">15.0</entry>
		<entry key="gridW">70</entry>
		<entry key="gridH">70</entry>
		<entry key="discardTime">0</entry>
		<entry key="javaAgent">graphsearch.ForwardPeer</entry>

		<!-- Random waypoint configuration -->
		<entry key="mobilityModel">RandomWaypoint</entry>
		<entry key="minSpeed">1.0</entry>
		<entry key="maxSpeed">10.0</entry>
		<entry key="pauseTime">10.0</entry>

		<!-- Composition populator -->
		<entry key="populator">CompositionsPopulator</entry>
		<entry key="nCompositions">5</entry>
		<entry key="compositionIO">(1, 5)</entry>
		<entry key="dDistribution">((0.3, 5), (0.4, 4), (0.3, 2))</entry>
		<entry key="nDistribution">1.0</entry>
		<entry key="width">(1, 4)</entry>

		<!-- Search behavior -->
		<entry key="nodeBehavior">CompositionSearchBehavior</entry>
		<entry key="searchFreq">0.1</entry>
		<entry key="timeRange">(3.0, 'END')</entry>

		<!-- Peer configuration variables -->
		<entry key="messageProcessor.randomWait">200</entry>
		<entry key="beaconDetector.beaconTime">1000</entry>
		<entry key="reliableBroadcast.maxTries">3</entry>
		<entry key="dissemination.maxDistance">5</entry>
		<entry key="parameterSearch.searchMessageTTL">5</entry>
		<entry key="graphsearch.maxTTL">10</entry>
		<entry key="graphsearch.searchExpiration">20000</entry>
	</staticConfig>
	<dynamicConfig />
	<repeat number="1" />
	<measures>
		<measure type="generic.BroadcastedPackets" period="0.25" />
		<measure type="generic.ReceivedPackets" period="0.25" />
		<measure type="generic.BroadcastTime" period="0.25" />
		<measure type="detection.AvgNeighbors" period="0.25" />
		<measure type="generic.ReliableBroadcastedMessages" period="0.25" />
		<measure type="generic.RebroadcastedMessages" period="0.25" />
		<measure type="generic.DeliveredMessages" period="0.25" />
		<measure type="generic.ExpiredMessages" period="0.25" />
		<measure type="generic.DeliveredRatio" period="0.25" />
		<measure type="generic.DeliveringTime" period="0.25" />
		<measure type="graphsearch.StartedCompositions" period="0.25" />
		<measure type="graphsearch.FoundCompositionsRatio" period="0.25" />
		<measure type="graphsearch.AvgCompositionSearchTime" period="0.25" />
		<measure type="graphsearch.AvgCompositionAvailableTime" period="0.25" />
		<measure type="graphsearch.SentCompositionModificationMessages" period="0.25" />
		<measure type="graphsearch.SentFCompositionMessages" period="0.25" />
		<measure type="graphsearch.SentInvalidCompositionsMessages" period="0.25" />
		
		<measure type="generic.SentACKMessages" period="0.25" />
		<measure type="generic.ReceivedACKMessages" period="0.25" />
		
		<measure type="detection.SentBeaconMessages" period="0.25" />
		<measure type="dissemination.SentTableMessages" period="0.25" />
		
		<measure type="multicast.SentSearchMessages" period="0.25" />
		<measure type="multicast.SentRemoveParametersMessages" period="0.25" />
		<measure type="multicast.SentRemoveRouteMessages" period="0.25" />
		<measure type="multicast.SentSearchMessages" period="0.25" />
		<measure type="multicast.SentSearchResponseMessages" period="0.25" />
		<measure type="multicast.StartedSearches" period="0.25" />
		<measure type="multicast.RespondedSearches" period="0.25" />
		<measure type="multicast.AcceptedSearches" period="0.25" />
		<measure type="multicast.AcceptedSearchResponses" period="0.25" />
		<measure type="multicast.AcceptedSearchResponses" period="0.25" />
		
		<measure type="graphcreation.SentCollisionMessages" period="0.25" />
		<measure type="graphcreation.SentCollisionResponseMessages" period="0.25" />
		<measure type="graphcreation.SentConnectServicesMessages" period="0.25" />
		<measure type="graphcreation.SentDisconnectServicesMessages" period="0.25" />
		<measure type="graphcreation.SentForwardMessages" period="0.25" />
		<measure type="graphcreation.SentInhibeCollisionsMessages" period="0.25" />
		<measure type="graphcreation.SentRemovedServicesMessages" period="0.25" />
		<measure type="graphcreation.StartedCollisionMessages" period="0.25" />
	</measures>
</experiment>
