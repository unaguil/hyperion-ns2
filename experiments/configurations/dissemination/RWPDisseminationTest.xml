<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.5" type="% nodes with parameters">
	<staticConfig>
		<!-- Script configuration variables -->
		<entry key="nNodes">6</entry>
		<entry key="finishTime">10.0</entry>
		<entry key="gridW">100</entry>
		<entry key="gridH">100</entry>
		<entry key="discardTime">0</entry>
		<entry key="javaAgent">dissemination.Peer</entry>
		<!-- Random waypoint configuration -->
		<entry key="mobilityModel">RandomWaypoint</entry>-->
		<entry key="minSpeed">1.0</entry>
		<entry key="maxSpeed">25.0</entry>
		<entry key="pauseTime">10.0</entry>
		
		<!-- Parameter populator -->
		<entry key="populator">ParameterPopulator</entry>
		<entry key="nDistribution">((0.1, 5), (0.2, 3), (0.6, 2), (0.1, 0))</entry>
		<entry key="gDistribution">(0.2, 0.3, 0.4, 0.1)</entry>
		<entry key="tDistribution">('UNIQUE', (0.4, 0.6), (0.3, 0.4, 0.3), (0.1, 0.5, 0.4))</entry>
		
		<!-- Peer configuration -->
		<entry key="messageProcessor.randomWait">200</entry>
		<entry key="beaconDetector.beaconTime">1000</entry>
		<entry key="reliableBroadcast.rebroadcastTime">200</entry>
		<entry key="reliableBroadcast.maxTries">3</entry>

	</staticConfig>
	<dynamicConfig/>
	<repeat number="1"/>
	<measures>
		<measure type="generic.BroadcastedMessages" period="5.0"/>
		<measure type="generic.ReceivedMessages" period="5.0"/>
		<measure type="generic.SentBundleMessages" period="5.0"/>
		<measure type="generic.ReceivedBundleMessages" period="5.0"/>
		<measure type="generic.ReliableBroadcastedMessages" period="5.0"/>
		<measure type="generic.DeliveredMessages" period="5.0"/>
		<measure type="generic.ExpiredMessages" period="5.0"/>
		<measure type="generic.DeliveredRatio" period="5.0"/>
		<measure type="generic.SentACKMessages" period="5.0"/>
		<measure type="generic.ReceivedACKMessages" period="5.0"/>
		<measure type="detection.NewNeighborsDetected" period="5.0"/>
		<measure type="detection.LostNeighborsDetected" period="5.0"/>
		<measure type="detection.AvgNeighbors" period="5.0"/>
		<measure type="detection.SentBeaconMessages" period="5.0"/>
		<measure type="detection.ReceivedBeaconMessages" period="5.0"/>
		<measure type="dissemination.SentTableMessages" period="5.0"/>
		<measure type="dissemination.ReceivedTableMessages" period="5.0"/>
	</measures>
</experiment>
