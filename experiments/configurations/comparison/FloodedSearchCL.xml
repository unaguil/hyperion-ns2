<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.5" type="% nodes with parameters">
	<staticConfig>
		<import file="Common.xml" />		
		<entry key="javaAgent">floodsearch.Peer</entry>

		<!-- Peer configuration variables -->
		<entry key="floodsearch.msgInterval">1000</entry>
	</staticConfig>
	<dynamicConfig>
		<dynamicEntry key="compositionLength" interpolator="SetInterpolator" text="Composition Length [services]" values="(3, 5, 7, 9, 11)"/>
	</dynamicConfig>
	<repeat number="10" />
	<measures>
		<import file="AllMeasures.xml" />
	</measures>
</experiment>
