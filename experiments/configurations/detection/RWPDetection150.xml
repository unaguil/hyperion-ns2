<?xml version="1.0" encoding="UTF-8"?>
<experiment>
	<staticConfig>
		<!-- Script configuration variables -->
		<entry key="nNodes">150</entry>
		<entry key="finishTime">1000.0</entry>
		<entry key="gridW">742</entry>
		<entry key="gridH">742</entry>
		<entry key="discardTime">0</entry>
		<entry key="javaAgent">detection.Peer</entry>
		<!-- Random waypoint configuration -->
<!--		<entry key="mobilityModel">RandomWaypoint</entry>-->
		<entry key="mobilityModel">LoadMobilityModel</entry>
		<entry key="minSpeed">1.0</entry>
		<entry key="maxSpeed">25.0</entry>
		<entry key="pauseTime">10.0</entry>
		<!-- Peer configuration variables -->
		<entry key="reliableBroadcastPeer.maxTries">5</entry>
		<entry key="reliableBroadcastPeer.randomWait">150</entry>
		<entry key="beaconDetector.beaconTime">1000</entry>
		<entry key="beaconDetector.randomTime">200</entry>
	</staticConfig>
	<dynamicConfig/>
	<repeat number="40"/>
	<measures>
		<measure type="generic.BroadcastedMessages" period="5.0"/>
		<measure type="generic.ReceivedMessages" period="5.0"/>
		<measure type="detection.NewNeighborsDetected" period="5.0"/>
		<measure type="detection.LostNeighborsDetected" period="5.0"/>
		<measure type="detection.SentBeaconMessages" period="5.0"/>
		<measure type="detection.ReceivedBeaconMessages" period="5.0"/>
	</measures>
</experiment>
