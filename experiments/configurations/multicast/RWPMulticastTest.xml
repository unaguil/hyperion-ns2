<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.5" type="% nodes with parameters">
	<staticConfig>
		<!-- Script configuration variables -->
		<entry key="nNodes">6</entry>
		<entry key="finishTime">25.0</entry>
		<entry key="gridW">100</entry>
		<entry key="gridH">100</entry>
		<entry key="discardTime">0</entry>
		<entry key="javaAgent">multicast.peer.Peer</entry>
		
		<!-- Random waypoint configuration -->
		<entry key="mobilityModel">RandomWaypoint</entry>-->
		<entry key="minSpeed">1.0</entry>
		<entry key="maxSpeed">10.0</entry>
		<entry key="pauseTime">10.0</entry>
		
		<!-- Parameter populator -->
		<entry key="populator">ParameterPopulator</entry>
		<entry key="nDistribution">((0.1, 5), (0.2, 3), (0.6, 2), (0.1, 0))</entry>
		<entry key="gDistribution">(0.2, 0.3, 0.4, 0.1)</entry>
		<entry key="tDistribution">('UNIQUE', (0.4, 0.6), (0.3, 0.4, 0.3), (0.1, 0.5, 0.4))</entry>
		
		<!-- Search behavior -->
		<entry key="nodeBehavior">SearchBehavior</entry>
		<entry key="searchFreq">1.0</entry>
		<entry key="timeRange">('START', 3.0)</entry>
	</staticConfig>
	<dynamicConfig/>
	<repeat number="1"/>
	<measures>
		<measure type="multicast.AcceptedSearches" period="5.0"/>
		<measure type="multicast.AcceptedSearchResponses" period="5.0"/>
		<measure type="multicast.SentSearchMessages" period="5.0"/>
		<measure type="multicast.AvgAvailableTime" period="5.0"/>
		<measure type="multicast.AvgSearchTime" period="5.0"/>
		<measure type="multicast.RespondedSearches" period="5.0"/>
		<measure type="multicast.SentSearchResponseMessages" period="5.0"/>
		<measure type="multicast.SentRemoveParametersMessages" period="5.0"/>
		<measure type="multicast.SentRemoveRouteMessages" period="5.0"/>
		<measure type="multicast.AvgFoundParameters" period="5.0"/>
		<measure type="multicast.StartedSearches" period="5.0"/>
	</measures>
</experiment>
