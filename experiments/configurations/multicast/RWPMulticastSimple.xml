<?xml version="1.0" encoding="UTF-8"?>
<experiment tag="0.5" type="% nodes with parameters">
	<staticConfig>
		<!-- Script configuration variables -->
		<entry key="nNodes">6</entry>
		<entry key="finishTime">25.0</entry>
		<entry key="gridW">100</entry>
		<entry key="gridH">100</entry>
		<entry key="discardTime">0</entry>
		<entry key="javaAgent">multicast.peer.Peer</entry>
		
		<!-- Random waypoint configuration -->
		<entry key="mobilityModel">RandomWaypoint</entry>-->
		<entry key="minSpeed">1.0</entry>
		<entry key="maxSpeed">10.0</entry>
		<entry key="pauseTime">10.0</entry>
		
		<!-- Parameter populator -->
		<entry key="populator">ParameterPopulator</entry>
		<entry key="nDistribution">((1.0, 5), (0.0, 0))</entry>
		<entry key="gDistribution">(1.0,)</entry>
		<entry key="tDistribution">('UNIQUE',)</entry>
		
		<!-- Search behavior -->
		<entry key="nodeBehavior">SearchBehavior</entry>
		<entry key="searchFreq">1.0</entry>
		<entry key="timeRange">('START', 3.0)</entry>
		
		<!-- Peer configuration variables -->
		<entry key="reliableBroadcastPeer.delayedInit">0</entry>
		<entry key="reliableBroadcastPeer.maxTries">5</entry>
		<entry key="beaconDetector.beaconTime">2000</entry>
		<entry key="reactivePTableUPdate.maxDistance">10</entry>
		<entry key="parameterSearch.searchMessageTTL">10</entry>
	</staticConfig>
	<dynamicConfig/>
	<repeat number="1"/>
	<measures>
		<measure type="generic.SentMessages" period="0.5"/>
		<measure type="generic.ReceivedMessages" period="0.5"/>
		<measure type="detection.AvgNeighbors" period="0.5"/>
		<measure type="detection.NewNeighborsDetected" period="0.5"/>
		<measure type="detection.LostNeighborsDetected" period="0.5"/>
		<measure type="detection.SentBeaconMessages" period="0.5"/>
		<measure type="detection.ReceivedBeaconMessages" period="0.5"/>
		<measure type="dissemination.SentTableMessages" period="0.5"/>
		<measure type="dissemination.ReceivedTableMessages" period="0.5"/>
		<measure type="multicast.SentSearchMessages" period="0.5"/>
		<measure type="multicast.SentSearchResponseMessages" period="0.5"/>
		<measure type="multicast.SentRemoveParametersMessages" period="0.5"/>
		<measure type="multicast.SentRemoveRoutesMessages" period="0.5"/>
		<measure type="multicast.StartedSearches" period="0.5"/>
		<measure type="multicast.AcceptedSearches" period="0.5"/>
		<measure type="multicast.RespondedSearches" period="0.5"/>
		<measure type="multicast.AcceptedSearchResponses" period="0.5"/>
		<measure type="multicast.AvgFoundParameters" period="0.5"/>
		<measure type="multicast.AvgSearchTime" period="0.5"/>
		<measure type="multicast.AvgAvailableTime" period="0.5"/>
	</measures>
</experiment>
